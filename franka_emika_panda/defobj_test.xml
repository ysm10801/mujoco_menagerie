<mujoco>
  <option timestep="0.0005"/>
  <extension>
    <plugin plugin="mujoco.elasticity.solid"/>
  </extension>
  <worldbody>
    <flexcomp type="grid" count="12 2 2" spacing="0.1 0.1 0.1" pos=".9 0.0 0.3"
             radius=".0" rgba=".0 .7 .7 1" name="softbody" dim="3" mass="0.02">
      <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="auto"/>
      <edge damping="1"/>
      <plugin plugin="mujoco.elasticity.solid">
          <config key="poisson" value="0.2"/>
          <!--Units are in Pa (SI)-->
          <config key="young" value="5E4"/>
      </plugin>
    </flexcomp>
  </worldbody>
</mujoco>